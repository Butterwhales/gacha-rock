<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gacha_rock">

    <application
        android:name="com.example.gacha_rock.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/rock_chan_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rock_chan_icon"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gacharock">
        <activity
            android:name=".summon"
            android:screenOrientation="portrait" />
        <activity
            android:name=".store"
            android:screenOrientation="portrait" />
        <activity
            android:name=".settings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".inventory"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>